{
  "name": "@sapour/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "lsof -ti:3001 | xargs kill -9 2>/dev/null || true && ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:contracts": "jest --config ./jest.config.ts --testPathPattern=backend/test/contracts",
    "test:integration": "jest --config ./jest.config.ts --testPathPattern=backend/test/integration"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.7",
    "@nestjs/core": "^10.4.7",
    "@nestjs/platform-express": "^10.4.7",
    "@prisma/client": "^5.19.0",
    "@sentry/node": "^8.30.0",
    "@supabase/supabase-js": "^2.57.4",
    "dotenv": "^16.4.5",
    "googleapis": "^140.0.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "nestjs-zod": "^3.0.0",
    "openai": "^4.56.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.20",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.19.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
